{
  "compilerOptions": {
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "target": "es5",
    "lib": [
      "dom",
      "es7"
    ],
    "types": [
      "node",
      "jquery",
      "mocha"
    ]
  },
  "exclude": [
    "node_modules",
    "lib",
    "es"
  ]
}
